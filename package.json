{
  "name": "bartending2u-scheduler",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development tsx watch server/src/index.ts",
    "start:server": "node server/dist/index.js",
    "build:server": "tsc -p server/tsconfig.json",
    "prisma:generate": "prisma generate --schema server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema server/prisma/schema.prisma",
    "db:seed": "tsx server/prisma/seed.ts",
    "test:server": "npm run build:server && env MOCK_PRISMA=true NODE_ENV=test DATABASE_URL=postgresql://localhost:5432/dummy node server/test/employees.test.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "prisma": "^5.16.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.16.2",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.4"
}
